{
  "name": "tailwind-css-cheat-sheet-chrome-extension",
  "title": "Tailwind CSS Cheat Sheet",
  "description": "A chrome extension for finding quickly all the class names and CSS properties with this interactive cheat sheet.",
  "private": true,
  "version": "0.0.5",
  "type": "module",
  "scripts": {
    "dev": "pnpm run generate-types && concurrently \"tsx watch manifest.ts\" \"vite\"",
    "build": "pnpm run generate-types && tsc && vite build && pnpm run build:manifest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:manifest": "tsx manifest.ts",
    "generate-types": "make_types -i src/__generated__/generatedTypes.ts src/data/cheatsheet3.0.24.json CategoryCollection",
    "build:zip": "pnpm run build && tsx zip.ts",
    "export": "yarn run generate-types && tsc && vite build && yarn run build:manifest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/chrome": "^0.0.263",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "maketypes": "^1.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.6"
  }
}
